model person (
	key pk
	field pk serial64
	field name text

	field value          json
	field value_up       json (updatable)
	field value_null     json (nullable)
	field value_null_up  json (nullable, updatable)
)

create person (  )

read one ( select person, where person.pk = ? )

update person (
	where person.pk = ?
)

delete person (
	where person.pk = ?
)